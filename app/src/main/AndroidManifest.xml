<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="oktalkassignmentapp.sumit.com.oktalkassignmentapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        tools:replace="icon, label"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.OkTalkAssigmentMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.Assignment1Activity"/>
        <activity android:name=".activity.Assignment2Activity"/>

        <activity android:name="ar.com.daidalos.afiledialog.FileChooserActivity" />

    </application>

</manifest>